[mysqld]
skip-host-cache
skip-name-resolve

server-id                = 445566
log_bin                  = mysql-bin
expire_logs_days         = 1
binlog_format            = row
enforce_gtid_consistency = ON
gtid_mode                = ON
log_slave_updates        = ON
default-time-zone        = "+07:00"
default-authentication-plugin = mysql_native_password
